            <com.vn.ntesco.views.ReportImageView
                android:id="@+id/imageProcessed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llProcessingButton"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="@dimen/margin_big"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <com.vn.ntesco.views.NTesCoTextView
                android:id="@+id/btnProcessingStatus"
                style="@style/ntesco_text_view_style"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="@dimen/margin_large"
                android:background="@drawable/bg_button_selector"
                android:minWidth="@dimen/width_button"
                android:paddingLeft="@dimen/padding_big"
                android:paddingTop="@dimen/padding_smallXX"
                android:paddingRight="@dimen/padding_big"
                android:paddingBottom="@dimen/padding_smallXX"
                android:layout_marginTop="@dimen/margin_big"
                android:text="@string/processing_status_text"
                android:textAlignment="center"
                android:textColor="@drawable/text_color_button_selector"
                app:NTesCoTextViewFont="Bold" />

            <com.vn.ntesco.views.NTesCoTextView
                android:id="@+id/btnProcessed"
                style="@style/ntesco_text_view_style"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/bg_button_selector"
                android:minWidth="@dimen/width_button"
                android:paddingLeft="@dimen/padding_big"
                android:paddingTop="@dimen/padding_smallXX"
                android:paddingRight="@dimen/padding_big"
                android:paddingBottom="@dimen/padding_smallXX"
                android:layout_marginTop="@dimen/margin_big"
                android:text="@string/processed_text"
                android:textAlignment="center"
                android:textColor="@drawable/text_color_button_selector"
                app:NTesCoTextViewFont="Bold" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/llProductProcessed"
            android:layout_width="match_parent"
            android:layout_marginHorizontal="@dimen/margin_big"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            android:visibility="gone"
            android:layout_height="wrap_content">
            <com.vn.ntesco.views.NTesCoEditText
                android:id="@+id/edtNote"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:layout_marginLeft="@dimen/margin_normal"
                android:layout_marginRight="@dimen/margin_normal"
                android:layout_marginBottom="@dimen/margin_big"
                android:background="@drawable/bg_corner_grey"
                android:gravity="top"
                android:textColor="@color/grey_black"
                android:hint="@string/note"
                android:minHeight="@dimen/h_edit_text_multi_line"
                android:paddingLeft="@dimen/padding_normal"
                android:paddingTop="@dimen/padding_normal"
                android:paddingRight="@dimen/padding_normal"
                android:paddingBottom="@dimen/padding_normal"
                android:textSize="@dimen/font_normal" />

            <com.vn.ntesco.views.NTesCoTextView
                android:id="@+id/labelUpload"
                style="@style/ntesco_text_view_style"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/margin_normal"
                android:layout_marginRight="@dimen/margin_normal"
                android:layout_marginBottom="@dimen/margin_big"
                android:text="@string/upload_image_processing"
                android:textColor="@color/black_light"
                android:textSize="@dimen/font_normal"
                app:NTesCoTextViewFont="Regular" />

            <com.vn.ntesco.views.ReportImageView
                android:id="@+id/imageProcessingView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/llCommentCustomer"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/margin_big"
            android:orientation="vertical"
            android:paddingLeft="@dimen/padding_big"
            android:paddingRight="@dimen/padding_big">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_big"
                android:gravity="center_vertical">

                <View
                    android:layout_width="@dimen/width_line_label"
                    android:layout_height="match_parent"
                    android:background="#f1a707"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                </View>

                <com.vn.ntesco.views.NTesCoTextView
                    style="@style/ntesco_text_view_style"
                    android:layout_marginLeft="@dimen/margin_smallXX"
                    android:text="@string/review_comment"
                    android:textColor="@color/black_light"
                    android:textSize="@dimen/font_16"
                    app:NTesCoTextViewFont="SemiBold"
                    app:layout_constraintTop_toTopOf="parent" />
            </LinearLayout>
            <com.vn.ntesco.views.NTesCoEditText
                android:id="@+id/edtComment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:layout_marginLeft="@dimen/margin_normal"
                android:layout_marginRight="@dimen/margin_normal"
                android:layout_marginBottom="@dimen/margin_big"
                android:background="@drawable/bg_corner_grey_selector"
                android:gravity="top"
                android:textColor="@drawable/text_color_black"
                android:hint="@string/hint_review_comment"
                android:minHeight="@dimen/h_edit_text_multi_line"
                android:paddingLeft="@dimen/padding_normal"
                android:paddingTop="@dimen/padding_normal"
                android:paddingRight="@dimen/padding_normal"
                android:paddingBottom="@dimen/padding_normal"
                android:textSize="@dimen/font_normal" />


        </LinearLayout>
        <com.vn.ntesco.views.NTesCoTextView
            android:id="@+id/btnConfirm"
            style="@style/ntesco_text_view_style"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/bg_button_orange"
            android:minWidth="@dimen/width_button"
            android:paddingLeft="@dimen/padding_big"
            android:paddingTop="@dimen/padding_smallXX"
            android:paddingRight="@dimen/padding_big"
            android:paddingBottom="@dimen/padding_smallXX"
            android:layout_marginTop="@dimen/margin_big"
            android:layout_marginBottom="@dimen/margin_large"
            android:text="@string/confirm"
            android:textAlignment="center"
            android:textColor="@color/white"
            app:NTesCoTextViewFont="Bold" />
    </LinearLayout>
</androidx.core.widget.NestedScrollView>
