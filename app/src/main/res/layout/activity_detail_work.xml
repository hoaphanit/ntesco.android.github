    android:id="@+id/scrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.DetailWorkActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/margin_big"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/margin_big"
            android:paddingRight="@dimen/margin_big">

            <ImageView
                app:layout_constraintTop_toTopOf="parent"
                android:id="@+id/newStatus"
                app:layout_constraintLeft_toLeftOf="@+id/new_text"
                app:layout_constraintRight_toRightOf="@+id/new_text"
                android:layout_width="@dimen/edit_icon"
                android:layout_height="@dimen/edit_icon"
                android:src="@mipmap/new_process" />

            <ImageView
                android:layout_width="0dp"
                android:background="@drawable/dotted"
                app:layout_constraintLeft_toRightOf="@+id/newStatus"
                app:layout_constraintRight_toLeftOf="@+id/confirm"
                app:layout_constraintBottom_toBottomOf="@+id/newStatus"
                app:layout_constraintTop_toTopOf="@+id/newStatus"
                android:layout_height="1dp"/>

            <ImageView
                android:layout_width="0dp"
                android:background="@drawable/dotted"
                app:layout_constraintLeft_toRightOf="@+id/confirm"
                app:layout_constraintRight_toLeftOf="@+id/processing"
                app:layout_constraintBottom_toBottomOf="@+id/newStatus"
                app:layout_constraintTop_toTopOf="@+id/newStatus"
                android:layout_height="1dp"/>

            <ImageView
                android:layout_width="0dp"
                android:background="@drawable/dotted"
                app:layout_constraintLeft_toRightOf="@+id/processing"
                app:layout_constraintRight_toLeftOf="@+id/completed"
                app:layout_constraintBottom_toBottomOf="@+id/newStatus"
                app:layout_constraintTop_toTopOf="@+id/newStatus"
                android:layout_height="1dp"/>


            <com.vn.ntesco.views.NTesCoTextView
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/newStatus"
                android:layout_width="0dp"
                android:text="@string/new_text"
                android:gravity="center"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintWidth_percent=".25"
                android:layout_height="wrap_content"
                android:id="@+id/new_text"
                />
            <ImageView
                android:id="@+id/confirm"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="@+id/confirmed_text"
                app:layout_constraintRight_toRightOf="@+id/confirmed_text"
                android:layout_width="@dimen/edit_icon"
                android:layout_height="@dimen/edit_icon"
                android:src="@mipmap/confirmed" />
            <com.vn.ntesco.views.NTesCoTextView
                app:layout_constraintTop_toBottomOf="@+id/confirm"
                android:layout_width="0dp"
                android:gravity="center"
                app:layout_constraintStart_toEndOf="@+id/new_text"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintWidth_percent=".25"
                android:text="@string/confirmed_text"
                android:layout_height="wrap_content"
                android:id="@+id/confirmed_text"
                />

            <ImageView
                android:id="@+id/processing"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="@+id/processsing_text"
                app:layout_constraintRight_toRightOf="@+id/processsing_text"
                android:layout_width="@dimen/edit_icon"
                android:layout_height="@dimen/edit_icon"
                android:src="@mipmap/processing" />
            <com.vn.ntesco.views.NTesCoTextView
                app:layout_constraintStart_toEndOf="@+id/confirmed_text"

                app:layout_constraintTop_toBottomOf="@+id/processing"
                android:layout_width="0dp"
                android:gravity="center"

                android:text="@string/processing_text"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintWidth_percent=".25"
                android:layout_height="wrap_content"
                android:id="@+id/processsing_text"
                />
            <ImageView
                android:id="@+id/completed"
                app:layout_constraintTop_toTopOf="parent"

                app:layout_constraintLeft_toLeftOf="@+id/completed_text"
                app:layout_constraintRight_toRightOf="@+id/completed_text"
                android:layout_width="@dimen/edit_icon"
                android:layout_height="@dimen/edit_icon"
                android:src="@mipmap/completed" />

            <com.vn.ntesco.views.NTesCoTextView
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/completed"
                android:layout_width="0dp"
                android:gravity="center"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintWidth_percent=".25"
                android:text="@string/completed_text"
                android:layout_height="wrap_content"
                android:id="@+id/completed_text"
                />


        </androidx.constraintlayout.widget.ConstraintLayout>




